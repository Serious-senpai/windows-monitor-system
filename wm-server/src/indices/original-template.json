{
    "index_patterns": [
        "logs-endpoint.events.windows-monitor-original-*"
    ],
    "data_stream": {},
    "priority": 1000,
    "template": {
        "mappings": {
            "dynamic": "strict",
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "event": {
                    "type": "object",
                    "dynamic": "strict",
                    "properties": {
                        "guid": {
                            "type": "keyword"
                        },
                        "raw_timestamp": {
                            "type": "long"
                        },
                        "process_id": {
                            "type": "long"
                        },
                        "thread_id": {
                            "type": "long"
                        },
                        "event_id": {
                            "type": "integer"
                        },
                        "opcode": {
                            "type": "integer"
                        },
                        "data": {
                            "type": "object",
                            "dynamic": "strict",
                            "properties": {
                                "type": {
                                    "type": "keyword"
                                },
                                "data": {
                                    "type": "object",
                                    "dynamic": "strict",
                                    "properties": {
                                        "file_object": {
                                            "type": "long"
                                        },
                                        "file_name": {
                                            "type": "keyword"
                                        },
                                        "image_base": {
                                            "type": "long"
                                        },
                                        "image_size": {
                                            "type": "long"
                                        },
                                        "process_id": {
                                            "type": "long"
                                        },
                                        "image_checksum": {
                                            "type": "long"
                                        },
                                        "image_file_name": {
                                            "type": "keyword"
                                        },
                                        "command_line": {
                                            "type": "text"
                                        },
                                        "unique_process_key": {
                                            "type": "long"
                                        },
                                        "parent_id": {
                                            "type": "long"
                                        },
                                        "session_id": {
                                            "type": "integer"
                                        },
                                        "exit_status": {
                                            "type": "integer"
                                        },
                                        "directory_table_base": {
                                            "type": "long"
                                        },
                                        "pid": {
                                            "type": "long"
                                        },
                                        "size": {
                                            "type": "integer"
                                        },
                                        "daddr": {
                                            "type": "ip"
                                        },
                                        "saddr": {
                                            "type": "ip"
                                        },
                                        "dport": {
                                            "type": "integer"
                                        },
                                        "sport": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "counter": {
                    "type": "long"
                },
                "eps": {
                    "type": "float"
                },
                "system": {
                    "type": "object",
                    "dynamic": "strict",
                    "properties": {
                        "os": {
                            "type": "object",
                            "dynamic": "strict",
                            "properties": {
                                "full": {
                                    "type": "keyword"
                                },
                                "kernel": {
                                    "type": "keyword"
                                },
                                "name": {
                                    "type": "keyword"
                                },
                                "platform": {
                                    "type": "keyword"
                                },
                                "version": {
                                    "type": "keyword"
                                }
                            }
                        },
                        "memory": {
                            "type": "object",
                            "dynamic": "strict",
                            "properties": {
                                "total_mem": {
                                    "type": "long"
                                },
                                "used_mem": {
                                    "type": "long"
                                },
                                "total_swap": {
                                    "type": "long"
                                },
                                "used_swap": {
                                    "type": "long"
                                }
                            }
                        },
                        "cpus": {
                            "type": "nested",
                            "dynamic": "strict",
                            "properties": {
                                "brand": {
                                    "type": "keyword"
                                },
                                "usage": {
                                    "type": "float"
                                },
                                "frequency": {
                                    "type": "integer"
                                },
                                "name": {
                                    "type": "keyword"
                                },
                                "vendor_id": {
                                    "type": "keyword"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}